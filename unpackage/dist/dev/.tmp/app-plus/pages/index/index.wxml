<view><scroll-view class="tabBars" scroll-x scroll-into-view="{{id}}" scroll-with-animation="true"><block wx:for="{{tabList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view id="{{'tab'+index}}" data-event-opts="{{[['tap',[['changeTabBars',[index]]]]]}}" bindtap="__e"><text class="{{[tabIndex==index?'active':'']}}">{{item.name}}</text></view></block></block></scroll-view><swiper style="{{('height:'+screenH)}}" current="{{tabIndex}}" duration="{{100}}" data-event-opts="{{[['change',[['changeSwiper',['$event']]]]]}}" bindchange="__e"><block wx:for="{{tabItem}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><scroll-view style="{{('height:'+screenH)}}" scroll-y="true" data-event-opts="{{[['scrolltolower',[['loadMore',[index]]]]]}}" bindscrolltolower="__e"><block wx:for="{{item.list}}" wx:for-item="tab" wx:for-index="listIndex" wx:key="listIndex"><block><block wx:if="{{tab.type=='swiperUrl'}}"><swiper-c vue-id="{{'1-'+index+'-'+listIndex}}" swiperUrl="{{tab.data}}" bind:__l="__l"></swiper-c></block><block wx:else><block wx:if="{{tab.type=='navList'}}"><nav vue-id="{{'2-'+index+'-'+listIndex}}" navList="{{tab.data}}" bind:__l="__l"></nav><cut-line vue-id="{{'3-'+index+'-'+listIndex}}" bind:__l="__l"></cut-line></block><block wx:else><block wx:if="{{tab.type=='threeAdv'}}"><three-adv vue-id="{{'4-'+index+'-'+listIndex}}" threeAdv="{{tab.data}}" bind:__l="__l"></three-adv><cut-line vue-id="{{'5-'+index+'-'+listIndex}}" bind:__l="__l"></cut-line></block><block wx:else><block wx:if="{{tab.type=='productList'}}"><list vue-id="{{'6-'+index+'-'+listIndex}}" productList="{{tab.data}}" bind:__l="__l"></list></block></block></block></block></block></block></scroll-view></swiper-item></block></swiper></view>